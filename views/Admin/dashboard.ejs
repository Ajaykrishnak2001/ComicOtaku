        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Dashboard </h2>
                    
                </div>
                
            </div>
           
            <div class="row">
                <div class="col-xl-8 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <span>
                            <h5 class="card-title">Sale statistics</h5>
                            <select style="max-width: 200px;" name="selection" class="form-select" id="selection" >
                                <option value="Daily">Last 30 days</option>
                                <option value="Monthly">Monthly</option>
                            </select>
                            </span>
                            
                        
                            <canvas id="myChart" height="120px"></canvas>
                        </article>
                    </div>
                   
                         
                <div class="col-xl-4 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Best Selling Categories</h5>
                           
                                <% categories.forEach(category => { %>
                                  <option value="<%= category._id %>"><%= category.cName %><br></option>
                                <% }) %>
                           
                        </article>
                    </div>
                </div>



                <div class="col-lg-7">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Best Selling Brands</h5>
                            <div class="new-member-list">
                                <div class="d-flex align-items-center justify-content-between mb-4">
                                    <div class="d-flex align-items-center">
                                        <div>
                                            <% const brandPopularity = {}; %>
                                            <% products.forEach(product => { %>
                                                <% const brand = product.brand; %>
                                                <% brandPopularity[brand] = (brandPopularity[brand] || 0) + 1; %>
                                            <% }); %>
                                            <% const uniqueBrands = Object.keys(brandPopularity).sort((a, b) => brandPopularity[b] - brandPopularity[a]); %>
                                            <% uniqueBrands.forEach(brand => { %>
                                                <div class="card mb-4">
                                                    <article class="card-body">
                                                        <h5 class="card-title"><%= brand %> </h5>
                                                    </article>
                                                </div>
                                            <% }); %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </article>
                    </div>
                </div>
                
                        
                    </div>
                </div>



                <div class="row">
                    <div class="col-lg-5">
                        <div class="card mb-4">
                            <article class="card-body">
                                <h5 class="card-title">Best Selling Products</h5>
                                <div class="new-member-list">
                                    <div class="d-flex align-items-center justify-content-between mb-4">
                                        <div class="d-flex align-items-center">
                                           
                                            <div>
                                                <% products.forEach(product => { %>
                                                    <div class="card mb-4">
                                                      <article class="card-body">
                                                        <h5 class="card-title"><%= product.pname %></h5>
                                                       
                                                      </article>
                                                    </div>
                                                  <% }) %>
                                            </div>
                                        </div>
                                        
                                    </div>
                                   
                                </div>
                            </article>
                        </div>
                    </div>

               
            </div>
            
          
