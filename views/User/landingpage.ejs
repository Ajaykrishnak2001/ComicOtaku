                <div class="grid-products grid--view-items">
                    <div class="row">
                        <% let count = 0; %>
                        <% products.forEach(product => { %>
                            <% if (count < 8) { %>
                                <div class="col-6 col-sm-6 col-md-3 col-lg-3 item">
                                    <!-- Start product item -->
                                    <div class="product-item">
                                        <!-- start product image -->
                                        <div class="product-image">
