                                                <!-- End primary image -->
                                                <!-- Hover image -->
                                                <img class="hover blur-up lazyloaded" data-src="<%= product.images[1] %>" src="/productAssets/<%= product.images[1] %>" alt="<%= product.pname %>" title="">
                                                <!-- End hover image -->
                                                <!-- Product labels -->
                                                <div class="product-labels rectangular">
                                                    <% if (product.discount > 0) { %>
                                                        <span class="lbl on-sale">-<%= product.discount %>%</span>
                                                    <% } %>
                                                    <% if (product.isNew) { %>
                                                        <span class="lbl pr-label1">New</span>
                                                    <% } %>
                                                </div>
                                                <!-- End product labels -->
                                            </a>
                                            
                                        </div>
                                        <!-- End product image -->
            
                                        <!-- Product details -->
                                        <div class="product-details text-center">
                                            <!-- Product name -->
                                            <div class="product-name">
                                                <a href="/products/<%= product._id %>"><%= product.pname %></a> <!-- Updated anchor tag -->
                                            </div>
                                            <div class="product-brand">
                                                <a href="/products/<%= product._id %>"><%= product.brand %></a> <!-- Updated anchor tag -->
                                            </div>
                                            <!-- End product name -->
                                            <!-- Product price -->
                                            <div class="product-price">
                                                <span class="price"><p> &#8377;<%= product.price %></p>
                                                    </span>
                                            </div>
                                            <!-- End product price -->
                                            <!-- Additional details or variants can go here -->
                                        </div>
                                        <!-- End product details -->
                                    </div>
                                    <!-- End product item -->
                                </div>
                                <% count++; %>
                            <% } %>
                        <% }); %>
                    </div>
                </div>
            </div>
        </div>
        
        
