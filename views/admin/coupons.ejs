        <body>
            <div class="container mt-5">
                <div class="row mb-4 align-items-center">
                    <div class="col-md-6">
                        <h2 class="mb-4 mt-4">Coupons</h2>
                    </div>
                    <div class="col-md-6 text-end">
                        <a href="/admin/createcoupon" class="btn btn-primary">Add Coupon</a>
                    </div>
                </div>
                <div class="table-responsive">
                    <table class="table table-bordered table-striped">
                        <thead class="table-dark">
                            <tr>
                                <th>Coupon ID</th>
                                <th>Description</th>
                                <th>Discount</th>
                                <th>Minimum Amount</th>
                                <th>Maximum Amount</th>
                                <th>Maximum User</th>
                                <th>Expire Date</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% coupons.forEach(coupon => { %>
                                <tr>
                                    <td><%= coupon.couponId %></td>
                                    <td><%= coupon.description %></td>
                                    <td><%= coupon.maximumDiscount %></td>
                                    <td><%= coupon.minimumAmount %></td>
                                    <td><%= coupon.maximumAmount %></td>
                                    <td><%= coupon.maximumUser %></td>
                                    <td><%= coupon.expireDate %></td>
                                    <td>
                                        <button class="btn btn-outline-primary" onclick="editCoupon('<%= coupon._id %>')">Edit Coupon</button>
                                        <button class="btn btn-outline-danger" onclick="deleteCoupon('<%= coupon._id %>')">Delete Coupon</button>
                                    </td>
                                </tr>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
                
                
                
